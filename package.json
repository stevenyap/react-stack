    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "eslint_d": "^4.2.5",
    "flow-bin": "^0.45.0",
    "flow-watch": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "react-test-renderer": "^15.5.4",
    "reactotron-react-js": "^1.10.0",
    "reactotron-redux": "^1.10.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
