  "author": "Steven Yap <stevenyap@futureworkz.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev.js",
    "watch:flow": "flow-watch",
    "flow": "flow",
    "watch:test": "jest --config .jestrc --watch",
    "test": "npm run lint && npm run flow && jest --config .jestrc",
    "lint": "eslint_d .",
    "build": "webpack -p --progress --config config/webpack.config.prod.js",
    "predeploy": "npm run test && npm run build",
    "deploy": "echo \"Error: No deployment specified\" && exit 1"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "ramda": "^0.23.0",
