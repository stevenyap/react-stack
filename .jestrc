{
  "setupFiles": [
    "<rootDir>/specs/specHelper.js"
  ],
  "globals": {
    "__DEV__": true
  },
  "collectCoverage": false,
  "coverageReporters": [
    "text"
  ],
  "testPathIgnorePatterns": [
    "/node_modules",
    "<rootDir>/config",
    "<rootDir>/public"
  ],
  "modulePaths": [
    "<rootDir>",
    "<rootDir>/src",
    "<rootDir>/specs"
  ]
}
